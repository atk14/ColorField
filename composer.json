        "keywords": ["color", "colors", "hexa", "rgb", "rgba", "field", "forms", "atk14"],
        "homepage": "https://github.com/atk14/ColorField",
        "license": ["MIT"],
        "authors": [
                {
                        "name": "ATK14 Development Team",
                        "homepage": "https://github.com/orgs/atk14/people"
                }
        ],
        "require": {
                "php": ">=5.3.0"
        },
        "require-dev": {
                "atk14/tester": "*",
                "atk14/forms": "dev-master",
                "atk14/core": "dev-master"
        },
        "autoload": {
                "classmap": ["src/app/fields"]
        }
